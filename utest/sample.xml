    <command>
      ls test-systemtast.cpp | wc -l
    </command>
    <expect>
      1
    </expect>
  </testcase>
  <testcase id="S2" description="system shell bad command">
    <command exitCode="2">
      ls nofile.null
    </command>
  </testcase>
</xml>
